{
    "prefix": "reproduce",
    "dataset": "cifar224",
    "memory_size": 2000,
    "memory_per_class": 20,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "der",
    "backbone_type": "vit_base_patch16_224",
    "device": ["0"],
    "seed": [1993],
    "iterative_temperature": 1.1,
    "adjust_lr": 1e-06,
    "ood_threshold": 1.0,
    "confidence_threshold": 0.7,
    "soften_labels": true,
    "adaptive_retention": true,
    "adaptive_correction": true,
    "entropy_loss": true,
    "cross_entropy_loss": true, 
    "hard_adjust": false,
    
    "init_epoch": 10,
    "init_lr": 1e-3,
    "init_milestones": [60, 120, 170],
    "init_lr_decay": 0.1,
    "init_weight_decay": 0.0005,
    "epochs": 10,
    "lrate": 1e-3,
    "milestones": [80, 120, 150],
    "lrate_decay": 0.1,
    "batch_size": 32,
    "arc_batch_size": 16,
    "weight_decay": 2e-4,
    "T": 2
}